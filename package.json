{
  "name": "llmexcel",
  "version": "1.0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/liminityab/llmexcel.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prestart": "npm run build",
    "prettier": "office-addin-lint prettier",
    "signin": "office-addin-dev-settings m365-account login",
    "signout": "office-addin-dev-settings m365-account logout",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "watch": "webpack --mode development --watch",
    "resize-icons": "./resize-icons.sh assets/logo-filled.png assets/",
    "deploy-production": "npm run build && touch dist/index.html && swa deploy --deployment-token $(cat DEPLOYMENT_TOKEN) dist --env production"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.1",
    "core-js": "^3.36.0",
    "openai": "^4.57.0",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@types/custom-functions-runtime": "^1.6.8",
    "@types/office-js": "^1.0.377",
    "@types/office-runtime": "^1.0.35",
    "acorn": "^8.5.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "custom-functions-metadata-plugin": "^1.7.0",
    "eslint-plugin-office-addins": "^3.0.2",
    "file-loader": "^6.2.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "office-addin-cli": "^1.6.3",
    "office-addin-debugging": "^5.1.3",
    "office-addin-dev-certs": "^1.13.3",
    "office-addin-lint": "^2.3.3",
    "office-addin-manifest": "^1.13.3",
    "office-addin-prettier-config": "^1.2.1",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "request": "^2.88.2",
    "source-map-loader": "^5.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^4.3.5",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "5.0.3"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
